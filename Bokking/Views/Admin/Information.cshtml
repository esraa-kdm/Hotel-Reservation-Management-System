@model IEnumerable<JoinTable>
@{
    ViewData["Title"] = "Information";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="table-responsive">
  <table class="table text-start align-middle table-bordered table-hover mb-0">
       <thead>
          <tr class="text-white">
                <th scope="col">Email</th>
                <th scope ="col"> User FirstName</th>
                <th scope="col"> User LastName</th>
                <th scope="col">Date Check-In</th>
                <th scope="col">Date Check-Out</th>
                <th scope="col">Hotel</th>
                <th scope="col">Room Type</th>
                <th scope="col">Event Name</th>
          </tr>

       </thead>
       <tbody>
          @foreach(var item in Model)
          {
            <tr class="text-white">
                <td>@item.userLogin.Email</td>
                <td>@item.customer.Fname</td>
                <td>@item.customer.Lname</td>
                <td>@item.reservation.CheckIn</td>
                <td>@item.reservation.CheckOut</td>
                <td>@item.hotel.Hotelname</td>
                <td>@item.room.Roomtype</td>
                <td>@item.service.Nameservice</td>

            </tr>
              
          }
       </tbody>

  </table>
</div>
